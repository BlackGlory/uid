{
  "private": true,
  "name": "uid",
  "version": "2.2.3",
  "main": "lib/index.js",
  "repository": "git@github.com:BlackGlory/uid.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "cross-env UID_HTTP2=true nodemon",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "start": "node lib/index.js",
    "start:pm2": "pm2 start lib/index.js --name 'chan' --wait-ready",
    "start:http2": "cross-env UID_HTTP2=true node lib/index.js",
    "docker:build": "docker build --tag uid .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "benchmark": "./benchmark/benchmark.sh",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.0",
    "@blackglory/jest-matchers": "^0.2.1",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "16",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "extra-fetch": "^1.8.2",
    "extra-request": "^3.0.1",
    "extra-response": "^0.3.8",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "jest-extended": "^1.0.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.1.0",
    "pm2": "^5.1.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@blackglory/go": "^0.1.0",
    "@blackglory/types": "^0.5.1",
    "extra-tags": "^0.2.2",
    "fastify": "^3.22.0",
    "fastify-cors": "^6.0.2",
    "fastify-metrics": "^8.0.0",
    "justypes": "^0.1.6",
    "nanoid": "^3.1.30",
    "prom-client": "^14.0.0",
    "uuid": "^8.3.2",
    "value-getter": "^0.2.1"
  }
}
