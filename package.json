{
  "private": true,
  "name": "uid",
  "version": "1.3.3",
  "main": "lib/index.js",
  "repository": "git@github.com:BlackGlory/uid.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage --config jest.config.js",
    "dev": "nodemon",
    "dev:http2": "UID_HTTP2=true nodemon",
    "clean": "rm -rf lib",
    "build": "cross-env NODE_ENV=production tsc --project tsconfig.build.json && tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "start": "cross-env NODE_ENV=production node lib/index.js",
    "start:http2": "cross-env NODE_ENV=production UID_HTTP2=true node lib/index.js",
    "docker:build": "docker build --tag uid .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm sut",
    "benchmark": "./benchmark/benchmark.sh",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test && yarn build",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "fastify": "^3.9.1",
    "fastify-cors": "^5.1.0",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.1.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
