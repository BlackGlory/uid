{
  "private": true,
  "name": "uid",
  "version": "3.0.0",
  "repository": "git@github.com:BlackGlory/uid.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk jest --runInBand",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.js",
    "dev": "nodemon",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag uid .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "bench": "ts-node --esm --require tsconfig-paths/register benches/index.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare clean lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/http-status": "^2.0.1",
    "@blackglory/jest-resolver": "^0.3.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^29.4.0",
    "@types/node": "16",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "extra-benchmark": "^0.2.2",
    "extra-fetch": "^4.0.4",
    "extra-request": "^8.1.6",
    "extra-response": "^0.5.1",
    "husky": "^4.3.8",
    "jest": "^29.4.1",
    "jest-resolve": "^29.4.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.3.1",
    "@fastify/cors": "^8.2.0",
    "extra-filesystem": "^0.4.10",
    "extra-tags": "^0.3.1",
    "fastify": "^4.12.0",
    "justypes": "^4.1.0",
    "nanoid": "^4.0.0",
    "semver": "^7.3.8",
    "uuid": "^9.0.0",
    "value-getter": "^0.3.0"
  }
}
